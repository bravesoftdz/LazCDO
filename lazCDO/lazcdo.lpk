<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="4">
    <PathDelim Value="\"/>
    <Name Value="lazCDO"/>
    <Type Value="RunAndDesignTime"/>
    <Author Value="LEWIS LOPEZ GOMEZ"/>
